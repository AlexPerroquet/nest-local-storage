{
	"name": "Python Poetry Development",
	"build": {
		"dockerfile": "Dockerfile",
		"context": ".."
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance"
			]
		}
	},
	"forwardPorts": [8000],
	"postCreateCommand": "poetry install",
	"remoteUser": "root"
}
